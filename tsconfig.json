{
    "compilerOptions": {
        "outDir": "./built",
        "allowJs": true,
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        "lib": ["ES2015", "esnext.asynciterable", "dom"]
    },
    "include": [
        "*.js",
        "*.ts",
        "*.jsx",
        "*.tsx",
        "**/*.jsx",
        "**/*.tsx",
        "**/*.js",
        "**/*.ts"
    ],
    "exclude": [
        "./built",
        "webpack.config.js"
    ]
}